<CGR
  xsi:noNamespaceSchemaLocation="classpath:xsd/cgr.xsd"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <type
    code="Country"
    label="Country"
    description="Country"
    visibility="PUBLIC"
    geometryType="POLYGON"
    isGeometryEditable="true">
    <attributes>
    </attributes>
  </type>  
  <type
    code="LocState"
    label="State"
    description="State"
    visibility="PUBLIC"
    geometryType="POLYGON"
    isGeometryEditable="true">
    <attributes>
    </attributes>
  </type>  
  <type
    code="County"
    label="County"
    description="County"
    visibility="PUBLIC"
    geometryType="POLYGON"
    isGeometryEditable="true">
    <attributes>
    </attributes>
  </type>  
  <type
    code="City"
    label="City"
    description="City"
    visibility="PUBLIC"
    geometryType="POLYGON"
    isGeometryEditable="true">
    <attributes>
    </attributes>
  </type>  
  <type
    code="Parcel"
    label="Parcel"
    description="Parcel"
    visibility="PUBLIC"
    geometryType="POLYGON"
    isGeometryEditable="true">
    <attributes>
    </attributes>
  </type>  
  <type
    code="TribalLands"
    label="Tribal Lands"
    description="Tribal Lands"
    visibility="PUBLIC"
    geometryType="POLYGON"
    isGeometryEditable="true">
    <attributes>
    </attributes>
  </type>  
  <type
    code="LocStructure"
    label="Structure"
    description="Structure location"
    visibility="PUBLIC"
    geometryType="POINT"
    isGeometryEditable="true">
    <attributes>
      <classification
        code="classificationType"
        label="Type"
        classificationType="Classification"
        root="Structure">
      </classification>
    </attributes>
  </type>
  <type
    code="RecreationArea"
    label="Recreation Area"
    description="Recreation Area locations"
    visibility="PUBLIC"
    geometryType="POLYGON"
    isGeometryEditable="true">
    <attributes>
      <classification
        code="classificationType"
        label="Type"
        classificationType="Classification"
        root="RecreationArea">
      </classification>
    </attributes>
  </type>  
  <type
    code="LandTransportation"
    label="Land Transportation"
    description="Land Transportation locations"
    visibility="PUBLIC"
    geometryType="LINE"
    isGeometryEditable="true">
    <attributes>
      <classification
        code="classificationType"
        label="Type"
        classificationType="Classification"
        root="LandTransportation">
      </classification>
    </attributes>
  </type>  
  <type
    code="WaterBody"
    label="Water Body"
    description="Water Body locations"
    visibility="PUBLIC"
    geometryType="POLYGON"
    isGeometryEditable="true">
    <attributes>
      <classification
        code="classificationType"
        label="Type"
        classificationType="Classification"
        root="WaterBody">
      </classification>
    </attributes>
  </type>  
  <type
    code="Watershed"
    label="Watershed"
    description="Watershed locations"
    visibility="PUBLIC"
    geometryType="POLYGON"
    isGeometryEditable="true">
    <attributes>
      <classification
        code="classificationType"
        label="Type"
        classificationType="Classification"
        root="WaterBody">
      </classification>
      <text
        code="hucNumber"
        label="HUC Number">
      </text>
    </attributes>
  </type>  
  <undirected-graph
    code="AdjacentTo"
    label="Adjacent To"
    description="Denotes adjacent locations"
  />
  <dag
    code="FlowsInto"
    label="Flows Into"
    description="Denotes water bodies that flow into each other"
  />
  <dag
    code="FlowsThrough"
    label="Flows Through"
    description="Denotes water bodies that flow through each other"
  />
  <undirected-graph
    code="ConnectedTo"
    label="Connected To"
    description="Denotes connected locations"
  />
  <undirected-graph
    code="VicinityOf"
    label="VicinityOf"
    description="Denotes locations within the vicinity of each other"
  />
  <dag
    code="ComponentOf"
    label="Component Of"
    description="Locations which are components of each other"
  />
  <undirected-graph
    code="Crosses"
    label="Crosses"
    description="Denotes locations which cross each other"
  />
  <hierarchy code="ADM_H" label="Administrative hierarchy" description="Administrative structure">
    <child code="Country">
      <child code="TribalLands"/>
      <child code="LocState">
        <child code="County">
          <child code="City">
            <child code="Parcel"/>
          </child>
        </child>
      </child>
    </child>
  </hierarchy>
  
</CGR>
