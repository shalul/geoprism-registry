<version
  xsi:noNamespaceSchemaLocation="classpath:com/runwaysdk/resources/xsd/version.xsd"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <doIt>
    <create>
      <mdBusiness
        name="net.geoprism.registry.ProgrammaticType"
        label="Programmatic Type"
        generateController="false"
        cacheAlgorithm="everything">
        <attributes>
          <char
            name="code"
            size="32"
            required="true"
            indexType="unique index"
            label="Code" />
          <reference
            name="mdVertex"
            type="com.runwaysdk.system.metadata.MdVertex"
            required="true"
            label="MdVertex" />
          <localChar
            name="displayLabel"
            label="Label"
            required="true" />
          <reference
            name="organization"
            label="Organization"
            required="false"
            type="net.geoprism.registry.Organization" />
        </attributes>
      </mdBusiness>
    </create>
    <permissions>
      <role roleName="geoprism.admin.Administrator">
        <grant>
          <mdBusinessPermission type="net.geoprism.registry.ProgrammaticType">
            <operation name="ALL" />
          </mdBusinessPermission>
        </grant>
      </role>
      <role roleName="cgr.RegistryMaintainer">
        <grant>
          <mdBusinessPermission type="net.geoprism.registry.ProgrammaticType">
            <operation name="ALL" />
          </mdBusinessPermission>
        </grant>
      </role>
    </permissions>    
  </doIt>
  <undoIt>
    <delete>
      <object
        key="net.geoprism.registry.ProgrammaticType"
        type="com.runwaysdk.system.metadata.MdBusiness"></object>
    </delete>
  </undoIt>
</version>