        <attributes>
          <date name="eventDate" label="Event Date" required="true" />
          <text name="typeCode" label="Type Code" required="true" />          
          <localCharEmbedded name="description" label="Label" required="false" />        
        </attributes>
      </mdVertex>       
      <mdVertex name="net.geoprism.registry.graph.Transition" label="Transition">
        <attributes>
          <text name="transitionType" label="Transition Type" required="true" />
