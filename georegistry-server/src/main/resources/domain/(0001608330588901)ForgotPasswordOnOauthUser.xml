<version xsi:noNamespaceSchemaLocation="classpath:com/runwaysdk/resources/xsd/version.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <doIt>
    <create>
      <mdException name="net.geoprism.registry.session.ForgotPasswordOnOauthUser" message="User [{username}] is configured to log in with the Oauth server [{oauthServer}]. You must log in via Oauth. If you have forgotten your password on the external system server, then you should submit your forgot password request to the external system server instead." >
        <attributes>
          <text name="username"/>
          <text name="oauthServer"/>
        </attributes>
      </mdException>
    </create>
    <update></update>
  </doIt>
  <undoIt>
    <delete></delete>
  </undoIt>
</version>