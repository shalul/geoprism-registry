    <createOrUpdate>

      <object type="com.runwaysdk.localization.LocalizedValueStore" key="change.request.email.implement.subject">
        <attribute name="storeTag" value="UIText" />
        <attribute name="storeKey" value="change.request.email.implement.subject" />
        <attributeStruct name="storeValue">
          <attribute name="defaultLocale" value="Change Request {status}" />
        </attributeStruct>
      </object>
      <object type="com.runwaysdk.localization.LocalizedValueStore" key="change.request.email.implement.body">
        <attribute name="storeTag" value="UIText" />
        <attribute name="storeKey" value="change.request.email.implement.body" />
        <attributeStruct name="storeValue">
          <attribute name="defaultLocale" value="Your change request for Geo-Object {geoobject} was implemented with status {status}. Click here to view the change request:\n{link}" />
        </attributeStruct>
      </object>
      
      <object type="com.runwaysdk.localization.LocalizedValueStore" key="change.request.email.submit.subject">
        <attribute name="storeTag" value="UIText" />
        <attribute name="storeKey" value="change.request.email.submit.subject" />
        <attributeStruct name="storeValue">
          <attribute name="defaultLocale" value="New Change Request" />
        </attributeStruct>
      </object>
      <object type="com.runwaysdk.localization.LocalizedValueStore" key="change.request.email.submit.body">
        <attribute name="storeTag" value="UIText" />
        <attribute name="storeKey" value="change.request.email.submit.body" />
        <attributeStruct name="storeValue">
          <attribute name="defaultLocale" value="A new change request has been submitted by {user} against Geo-Object {geoobject}. Click here to view the change request:\n{link}" />
        </attributeStruct>
      </object>

    </createOrUpdate>
