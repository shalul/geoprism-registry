<version xsi:noNamespaceSchemaLocation="classpath:com/runwaysdk/resources/xsd/version.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <doIt>
    <create>
      <mdException name="net.geoprism.registry.CodeLengthException"
                   label="Code Length Exception" 
                   message="The code attribute cannot exceed a length of {length} characters.">
        <attributes>
          <integer name="length"></integer>
        </attributes>
      </mdException>
    </create>
    <update></update>
  </doIt>
  <undoIt>
    <delete>
      <object key="net.geoprism.registry.CodeLengthException" type="com.runwaysdk.system.metadata.MdException"/>
    </delete>
  </undoIt>
</version>